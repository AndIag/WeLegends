<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="true" enabled="true" name="AddIndexes">
    <DisplayName>AddIndexes</DisplayName>
    <AssignTo createNew="false" transport="http" type="request"/>
    <Add source="request">
        <QueryParams>
            <QueryParam name="beginIndex">{urirequest.beginIndex}</QueryParam>
            <QueryParam name="endIndex">{urirequest.endIndex}</QueryParam>
        </QueryParams>
    </Add>
    <Set>
        <Verb>GET</Verb>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>